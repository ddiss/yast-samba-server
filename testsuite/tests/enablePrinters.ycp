{

// test for SambaServer::enablePrinters( enable )
// $Id$
// testedfiles: Samba.ycp SambaServer.ycp

    include "testsuite.ycp";
    
    import "Samba";

    TESTSUITE_INIT([
            $[
                "target" : $[
                    "tmpdir" : "/tmp"
                ]
            ],
            $[],
            $[
                "target" : $[
                    "bash_output" : $[]
                ]
            ]
        ],nil);    
    import "SambaServer";
    
    SambaServer::share_printers = false;
    SambaServer::shares[ "printers" ] = $[
   "comment": "All Printers",
   "path": "/var/tmp",
   "printable": true,
   "create mask": "0600",
   "browseable": false,
   "available": "yes",
   "guest ok": "no",
   "yast": true
    ];
    
    DUMP(SambaServer::share_printers);
    DUMP(SambaServer::shares);
    DUMP(SambaServer::modified);
    
    Samba::modified = false;
    TEST( ``( SambaServer::enablePrinters(true) ), [], nil );

    DUMP(SambaServer::share_printers);
    DUMP(SambaServer::shares);
    DUMP(SambaServer::modified);

    TEST( ``( SambaServer::enablePrinters(false) ), [], nil );

    DUMP(SambaServer::share_printers);
    DUMP(SambaServer::shares);
    DUMP(SambaServer::modified);
}
