{

// test for SambaServer::enableAllSystemPrinters()
// $Id$

    include "testsuite.ycp";
    
    import "SambaServer";
    
    SambaServer::printer_status = $[
	"system_lp1" : true,
	"system_lp2" : false,
	"lp3" : true,
	"lp4" : false
    ];
    
    SambaServer::system_printers = ["system_lp1", "system_lp2"];
    
    DUMP(SambaServer::share_printers);
    DUMP(SambaServer::printer_status);
    
    Samba::modified = false;
    TEST( ``( SambaServer::enableAllSystemPrinters() ), [], nil );

    DUMP(SambaServer::share_printers);
    DUMP(SambaServer::printer_status);
}
