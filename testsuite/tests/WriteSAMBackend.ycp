// test for SambaServerPassdb::Write()
// $Id$

    include "testsuite.ycp";

    TESTSUITE_INIT([
            $[
                "target" : $[
                    "tmpdir" : "/tmp"
                ]
            ],
            $[],
            $[
                "target" : $[
                    "bash_output" : $[]
                ]
            ]
        ],nil);    
    import "SambaServerPassdb";

    SambaServerPassdb::SetBackends ( [ "smbpasswd", "ldapsam:ldaps://ldap.example.com" ] );
    
    TEST( ``( SambaServerPassdb::Write() ), [
	$[],
	$[],
	$[ "target" : $[ "bash" : 0 ] ] 
    ], true );
    
    DUMP( SambaServerPassdb::Modified() );
}
