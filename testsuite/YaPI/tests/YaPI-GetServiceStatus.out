Return	Disabled service
Read	.target.tmpdir "/tmp"
Read	.target.stat "/etc/init.d/smb" nil
Execute	.target.bash "/sbin/chkconfig -l smb | /usr/bin/grep -q '[0-6]:on'" 1
Return	0
Return	Enabled service
Read	.target.stat "/etc/init.d/smb" nil
Execute	.target.bash "/sbin/chkconfig -l smb | /usr/bin/grep -q '[0-6]:on'" 0
Return	1
